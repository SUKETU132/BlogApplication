{
    "version": 2,
    "builds": [
        {
            "src": "vercel.sh",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "vercel.sh"
        }
    ],
    "env": {
        "VERCEL_URL": "@BlogApplication/BlogApplication.vercel.app"
    },
    "outputDirectory": "staticfiles_build"
}